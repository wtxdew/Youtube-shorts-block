{
    "manifest_version": 2,
    "default_locale": "en",
    
    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "1.0",
    
    "icons": {
        "16":"images/icon32.png",
        "48":"images/icon64.png",
        "128":"images/icon128.png"
    },
    
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "all_frames": true,
        "js": ["content.js"],
        "css": ["content.css"]
    }],
    
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16":"images/icon32.png",
            "48":"images/icon64.png",
            "128":"images/icon128.png"
        }
    },
    
    "web_accessible_resources": [
        {
            "resources": [
                "images/to_normal.svg"
            ],
            "matches": ["*://*.youtube.com/*"]
        }
    ],
    
    "permissions": [ ]
}
